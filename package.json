{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "main": "app/dist/app.js",
  "scripts": {
    "build-ts": "tsc -p app/tsconfig.json",
    "clean": "npm run res:clean && rm -rf app/dist && rm -rf app/public/javascripts",
    "build": "npm run build-ts && npm run res:build && npm run build:js",
    "start": "npm run serve",
    "serve": "node app/dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:all": "bsb -clean-world",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript build -w",
    "build:js": "webpack",
    "watch:js": "webpack-dev-server"
  },
  "dependencies": {
    "ejs": "^3.1.7",
    "express": "~4.18.2",
    "mobile-detect": "^1.4.5"
  },
  "devDependencies": {
    "rescript": "^10.0.1",
    "@types/express": "^4.17.15",
    "@types/node": "^11.15.54",
    "npm-run-all": "^4.1.3",
    "typescript": "^4.9",
    "webpack": "^5.75",
    "webpack-cli": "^5.0",
    "webpack-dev-server": "^4.11"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  }
}
